[alias]
	desc = show -s --format=oneline
	descs = log --format=oneline
	br = branch
	b = rebase
	ch = cherry-pick
	cl = clone
	k  = checkout
	l  = log --decorate --graph
	m  = merge
	pick = cherry-pick
	pl = pull
	ps = push
	rb = rebase
	s  = stash
	t = reset
	rh = reset --hard
[push]
	default = matching
[core]
	excludesfile = ~/.gitignore
[pull]
	rebase = true
