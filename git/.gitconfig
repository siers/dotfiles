[alias]
	br = branch
	b = rebase
	ch = cherry-pick
	cl = clone
	amend = commit --amend
	amn = commit --amend
	smush = commit --amend -C HEAD
	redo = commit --amend -C HEAD
	commit = commit -v
	desc = show -s --format=oneline
	descs = log --format=oneline
	k  = checkout
	l  = log --decorate --graph
	lo  = log --format=oneline
	m  = merge
	o = show
	fo = show --format=fuller
	p = cherry-pick
	pl = pull
	ps = push
	pf = push --force-with-lease
	rb = rebase
	rh = reset --hard
	r = reset
	s = stash
[push]
	default = current
[core]
	excludesfile = ~/.gitignore
[pull]
	rebase = true
